{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/breakout.html",
      "dest": "/public/breakout.html"
    },
    {
      "src": "/rhythm.html",
      "dest": "/public/rhythm.html"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}